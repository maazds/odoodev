<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="odoo16_list">
      <field name="name">odoo16 list</field>
      <field name="model">odoo.16</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="value"/>
        </tree>
      </field>
    </record>


    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="odoo16.action_window">
      <field name="name">odoo16 Dev</field>
      <field name="res_model">odoo.16</field>
      <field name="view_mode">tree,form</field>
    </record>


    <!-- server action to the one above -->
